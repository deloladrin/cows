<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="strokeColor" format="color" />
    <attr name="strokeWidth" format="float" />

    <declare-styleable name="ImageTextButton">
        <attr name="android:src" format="reference" />
        <attr name="android:textColor" format="color" />
        <attr name="android:textSize" format="dimension" />
        <attr name="android:text" format="string" />
    </declare-styleable>

    <declare-styleable name="CircleTextView">
        <attr name="strokeColor" />
        <attr name="strokeWidth" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="strokeColor" />
        <attr name="strokeWidth" />
    </declare-styleable>

    <declare-styleable name="ToggleImageView">
        <attr name="toggledColor" format="color" />
        <attr name="strokeWidth" />
    </declare-styleable>

    <declare-styleable name="LimitedScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DiagnosisContainer">
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StatusTemplateContainer">
        <attr name="disabledStrokeColor" format="color" />
        <attr name="strokeColor" />
        <attr name="strokeWidth" />
    </declare-styleable>

    <declare-styleable name="ResourceContainer">
    </declare-styleable>

</resources>